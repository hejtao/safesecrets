{
  "overrides": [
    {
      "files": ["*.jsx", "*.tsx"],
      "options": {
        "printWidth": 100,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "bracketSpacing": true,
        "bracketSameLine": false,
        "semi": true,
        "trailingComma": "es5",
        "tabWidth": 2
      }
    }
  ]
}
